{
  "name": "@slr-finance/contracts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build:for-frontend": "rm -rf ./dist && hardhat export-abi && json -A -I -f ./dist/abi/Multicall.json -e 'this[0].stateMutability=\"view\"' && typechain --target ethers-v5 --out-dir ./tmp-typechain-prebuild-ts './dist/abi/*.json' && tsup ./tmp-typechain-prebuild-ts/index.ts --format esm,cjs --dts && rm -rf ./tmp-typechain-prebuild-ts",
    "compile": "hardhat compile",
    "local-node": "hardhat node",
    "deploy": "hardhat deploy"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.5.0",
    "abdk-libraries-solidity": "3.0.0"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.6.8",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.2",
    "@nomicfoundation/hardhat-network-helpers": "1.0.3",
    "@nomicfoundation/hardhat-toolbox": "1.0.2",
    "@nomiclabs/hardhat-ethers": "2.1.0",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@primitivefi/hardhat-dodoc": "0.2.3",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.2",
    "@types/chai": "4.3.1",
    "@types/jest": "28.1.6",
    "@types/mocha": "9.1.1",
    "@types/node": "18.6.1",
    "chai": "4.3.6",
    "dotenv": "16.0.1",
    "ethers": "5.6.9",
    "hardhat": "2.10.1",
    "hardhat-abi-exporter": "2.10.0",
    "hardhat-deploy": "0.11.12",
    "hardhat-gas-reporter": "1.0.8",
    "hardhat-packager": "1.4.1",
    "json": "11.0.0",
    "mocha": "9.2.2",
    "renamer": "4.0.0",
    "ts-node": "10.9.1",
    "tsup": "6.2.0",
    "typechain": "8.1.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.2",
  "peerDependencies": {
    "@ethersproject/providers": "*",
    "ethers": "*"
  }
}
